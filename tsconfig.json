{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "target": "es2018",
    "module": "commonjs",
    "lib": ["es2018", "esnext.asynciterable"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "baseUrl": "src",
    "paths": {
      "api": ["./api"],
      "generated/type-graphql": ["../prisma/generated/type-graphql"]
    }
  },
  "include": ["**/*.ts", "../prisma/generated/**/*.ts", "../grpc/**/*.ts"]
}
